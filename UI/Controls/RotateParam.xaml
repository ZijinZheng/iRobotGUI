<src:BaseParamControl x:Class="iRobotGUI.Controls.RotateParam"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:src="clr-namespace:iRobotGUI.Controls"
		Height="180" Width="180" Background="White" 
		RenderOptions.BitmapScalingMode="HighQuality">
	<Grid>
		<Ellipse x:Name="ellipse">
			<Ellipse.Fill>
				<ImageBrush ImageSource="/iRobotGUI;component/pic/irobotcontrol.png"/>
			</Ellipse.Fill>
		</Ellipse>
		<Label x:Name="label" Content="" Margin="0" Width="40" HorizontalAlignment="Right" VerticalAlignment="Bottom" />
	</Grid>
</src:BaseParamControl>
