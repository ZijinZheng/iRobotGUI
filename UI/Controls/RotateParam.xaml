<src:BaseParamControl x:Class="iRobotGUI.Controls.RotateParam"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:src="clr-namespace:iRobotGUI.Controls"
		Height="180" Width="180" Background="White" 
		RenderOptions.BitmapScalingMode="HighQuality">
	<Grid>
		<Grid x:Name="RotateGrid">
			<Ellipse>
				<Ellipse.Fill>
					<ImageBrush ImageSource="/iRobotGUI;component/pic/irobotcontrol.png"/>
				</Ellipse.Fill>
			</Ellipse>
		</Grid>
		<Label x:Name="label" Content="" HorizontalAlignment="Right" Margin="0" Width="35" RenderTransformOrigin="0.241,-0.286" VerticalAlignment="Bottom"/>
	</Grid>
</src:BaseParamControl>
