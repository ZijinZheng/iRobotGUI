<Window x:Class="iRobotPrototypeWpf.SongWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:wf="clr-namespace:Sanford.Multimedia.Midi.UI;assembly=Sanford.Multimedia.Midi"
        Title="Song" Height="326" Width="648" Loaded="Window_Loaded">
    <Grid>
        <WindowsFormsHost Margin="0,138,0,54">
            <wf:PianoControl x:Name="pianoKeyboard" PianoKeyDown="pianoKeyboard_PianoKeyDown" LowNoteID="31" HighNoteID="127" PianoKeyUp="pianoKeyboard_PianoKeyUp" />
        </WindowsFormsHost>
        <TextBox x:Name="textBoxSong" HorizontalAlignment="Left" Height="81" Margin="97,25,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="450" VerticalScrollBarVisibility="Auto"/>
        <Button x:Name="ButtonOk" Content="OK" HorizontalAlignment="Left" Margin="188,265,0,0" VerticalAlignment="Top" Width="75" Click="ButtonOk_Click"/>
        <Button x:Name="ButtonClear" Content="Clear" HorizontalAlignment="Left" Margin="359,265,0,0" VerticalAlignment="Top" Width="75" Click="ButtonClear_Click"/>
    </Grid>
</Window>
